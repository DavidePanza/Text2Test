   "source": [
    "import pdftotext\n",
    "\n",
    "# Open the PDF file in binary mode\n",
    "with open(\"input.pdf\", \"rb\") as f:\n",
    "    pdf = pdftotext.PDF(f)\n",
    "\n",
    "# Join all pages' text and write to output file\n",
    "with open(\"output.txt\", \"w\", encoding=\"utf-8\") as f:\n",
    "    f.write(\"\\n\\n\".join(pdf))\n"
   ]
